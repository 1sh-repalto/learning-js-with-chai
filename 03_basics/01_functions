function totalPrice(...price){ // Rest operator - groups all the incoming arguments into one array
    console.log(price);
}
totalPrice(100, 200, 300, 400 ,500);

// Hoisiting - Hoisting is JavaScript's default behavior of moving all declarations to the top of the current scope
addNums(3, 5);
function addNums(num1, num2) {
    console.log(num1 + num2);
}